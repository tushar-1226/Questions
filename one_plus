def plusOne(digits):  #mehtod
    n = len(digits)
    for i in range(n-1,-1,-1):    #logic
        if digits[i] <9:
            digits[i] += 1
            return digits
        digits[i] = 0
        return [1] + digits
    
digits = [4,3,2,1]        #testcase_digits
print(plusOne(digits))    #call method
